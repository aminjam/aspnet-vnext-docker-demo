{
    "domains": {
        "consumer": "http://aspnet-todo-demo-api.com",
        "admin": "http://api.admin"
    },
    "endpoints": {
        "consumer": {
            "todoFind": {
                "method": "GET",
                "url": "/api/todo/{id}"
            },
            "todoDelete": {
                "method": "DELETE",
                "url": "/api/todo/{id}"
            },
            "todoCreate": {
                "method": "POST",
                "url": "/api/todo"
            },
            "todoUpdate": {
                "method": "PUT",
                "url": "/api/todo"
            }
        }
    }
}
